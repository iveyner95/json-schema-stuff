{
  "$id": "https://example.com/complex-object.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Complex Object",
  "type": "object",
  "properties": {
    "name": {
      "type": "string"
    },
    "age": {
      "type": "integer",
      "minimum": 0
    },
    "address": {
      "type": "object",
      "minProperties": 0,
      "maxProperties": 4,
      "properties": {
        "street": {
          "type": "string"
        },
        "city": {
          "type": "string"
        },
        "state": {
          "type": "string"
        },
        "postalCode": {
          "type": "string",
          "pattern": "\\d{5}"
        }
      },
      "required": ["street", "city", "state", "postalCode"]
    },
    "hobbies": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "arrayWithPropertyNamesObject": {
      "type": "array",
      "items": {
        "type": "object",
        "propertyNames": {
          "pattern": "^[A-Za-z_][A-Za-z0-9_]*$"
        }
      }
    },
    "testPatternPropertiesObj": {
      "type": "object",
      "patternProperties": {
        "^S_": {
          "type": "string"
        }
      }
    },
    "testAdditionalPropertiesWithValue": {
      "type": "object",
      "additionalProperties": {
        "type": "string"
      }
    },
    "testAdditionalPropertiesWithFalse": {
      "type": "object",
      "additionalProperties": false
    },
    "booleanType": {
      "type": "boolean"
    },
    "NullType": {
      "type": "null"
    },
    "NumberType": {
      "type": "number"
    }
  },
  "required": ["name", "age"]
}
